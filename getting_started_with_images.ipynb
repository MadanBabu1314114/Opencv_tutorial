    "import cv2 as cv\n",
    "import sys"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "img = cv.imread(cv.samples.findFile(\"image.jpg\"))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "if img is None :\n",
    "    sys.exit(\"Could not read the image\")\n",
    "\n",
    "cv.imshow(\"Display Window \" , img)\n",
    "k = cv.waitKey(0)\n",
    "if k == ord('s'):\n",
    "    cv.imwrite(\"image.jpg\" , img)\n",
    "    "
   ]
